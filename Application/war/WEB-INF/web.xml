<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>com.creative.full.cloudcontact.SignupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/HTML/signup</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>loginMappig</servlet-name>
		<servlet-class>com.creative.full.cloudcontact.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginMappig</servlet-name>
		<url-pattern>/HTML/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>contactus</servlet-name>
		<servlet-class>com.creative.full.cloudcontact.ContactUsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>contactus</servlet-name>
		<url-pattern>/JSP/contactus_modal</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>forgotpassword</servlet-name>
		<servlet-class>com.creative.full.cloudcontact.ForgotPassServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>forgotpassword</servlet-name>
		<url-pattern>/HTML/forgotpass</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>logoff</servlet-name>
		<servlet-class>com.creative.full.cloudcontact.LogOffServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logoff</servlet-name>
		<url-pattern>/JSP/logoff</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>resetpassword</servlet-name>
	<servlet-class>com.creative.full.cloudcontact.ResetPasswordServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>resetpassword</servlet-name>
		<url-pattern>/JSP/changepassword</url-pattern>
	</servlet-mapping>
</web-app>

